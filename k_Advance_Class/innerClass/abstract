# Abstract + Anonymous class

abstract class A {
    public abstract void show();
}

// class B extends A {

//     public void show() {
//         System.out.println("In B Show");
//     }
// }

public class abstract {
    public static void main(String[] a) {

        A obj = new A() {
            public void show() {
                System.out.println("In new Show")
            }
        };
        
        obj.show();

    }
}







